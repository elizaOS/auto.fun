{
    "license": "ISC",
    "scripts": {
        "lint:fix": "prettier */*.js \"*/**/*{.js,.ts}\" -w",
        "lint": "prettier */*.js \"*/**/*{.js,.ts}\" --check",
        "build": "RUSTUP_TOOLCHAIN=nightly anchor build",
        "clean": "RUSTUP_TOOLCHAIN=nightly anchor clean",
        "program-id:main": "RUSTUP_TOOLCHAIN=nightly anchor run --provider.cluster Mainnet ts-node scripts/getProgramId.ts",
        "program-id:dev": "RUSTUP_TOOLCHAIN=nightly anchor run --provider.cluster Devnet ts-node scripts/getProgramId.ts",
        "init_autofun:main": "RUSTUP_TOOLCHAIN=nightly anchor run --provider.cluster Mainnet init_autofun",
        "init_autofun:dev": "RUSTUP_TOOLCHAIN=nightly anchor run --provider.cluster Devnet init_autofun",
        "deploy:vault_dev": "RUSTUP_TOOLCHAIN=nightly anchor deploy --program-name raydium_vault --provider.cluster Devnet",
        "deploy:autofun_dev": "RUSTUP_TOOLCHAIN=nightly anchor deploy --program-name autofun --provider.cluster Devnet",
        "deploy:vault_main": "RUSTUP_TOOLCHAIN=nightly anchor deploy --program-name raydium_vault --provider.cluster Mainnet",
        "deploy:autofun_main": "RUSTUP_TOOLCHAIN=nightly anchor deploy --program-name autofun --provider.cluster Mainnet",
        "deploy_buffer:vault_main": "solana program deploy --buffer recover.json --url mainnet target/deploy/raydium_vault.so",
        "deploy_buffer:autofun_main": "solana program deploy --buffer recover.json --url mainnet target/deploy/autofun.so",
        "deploy_buffer:vault_dev": "solana program deploy --buffer recover.json --url devnet target/deploy/raydium_vault.so",
        "deploy_buffer:autofun_dev": "solana program deploy --buffer recover.json --url devnet target/deploy/autofun.so",
        "check_config:main": "RUSTUP_TOOLCHAIN=nightly anchor run --provider.cluster Mainnet check_config",
        "check_config:dev": "RUSTUP_TOOLCHAIN=nightly anchor run --provider.cluster Devnet check_config",
        "test:dev": "RUSTUP_TOOLCHAIN=nightly anchor run --provider.cluster Devnet test",
        "test:main": "RUSTUP_TOOLCHAIN=nightly anchor run --provider.cluster Mainnet test",
        "init:main": "RUSTUP_TOOLCHAIN=nightly anchor run --provider.cluster Mainnet test_init",
        "init:dev": "RUSTUP_TOOLCHAIN=nightly anchor run --provider.cluster Devnet test_init",
        "withdraw:main": "RUSTUP_TOOLCHAIN=nightly anchor run --provider.cluster Mainnet test_withdraw",
        "withdraw:dev": "RUSTUP_TOOLCHAIN=nightly anchor run --provider.cluster Devnet test_withdraw",
        "claim:main": "RUSTUP_TOOLCHAIN=nightly anchor run --provider.cluster Mainnet test_claim",
        "claim:dev": "RUSTUP_TOOLCHAIN=nightly anchor run --provider.cluster Devnet test_claim",
        "position:main": "RUSTUP_TOOLCHAIN=nightly anchor run --provider.cluster Mainnet test_nft_balance",
        "position:dev": "RUSTUP_TOOLCHAIN=nightly anchor run --provider.cluster Devnet test_nft_balance",
        "changeClaimer:main": "RUSTUP_TOOLCHAIN=nightly anchor run --provider.cluster Mainnet test_change_claimer",
        "changeClaimer:dev": "RUSTUP_TOOLCHAIN=nightly anchor run --provider.cluster Devnet test_change_claimer",
        "deposit:main": "RUSTUP_TOOLCHAIN=nightly anchor run --provider.cluster Mainnet test_deposit",
        "deposit:dev": "RUSTUP_TOOLCHAIN=nightly anchor run --provider.cluster Devnet test_deposit",
        "update_state:main": "RUSTUP_TOOLCHAIN=nightly anchor run --provider.cluster Mainnet update_state",
        "update_state:dev": "RUSTUP_TOOLCHAIN=nightly anchor run --provider.cluster Devnet update_state",
        "balance:main": "solana balance -um",
        "balance:dev": "solana balance -ud",
        "recover": "solana-keygen recover -o recover.json",
        "clear": "rm -r recover.json"
    },
    "dependencies": {
        "@coral-xyz/anchor": "^0.30.1",
        "@raydium-io/raydium-sdk-v2": "^0.1.90-alpha",
        "@solana/spl-token": "^0.4.13",
        "@solana/web3.js": "^1.87.6",
        "bip39": "^3.1.0",
        "bs58": "^5.0.0",
        "ed25519-hd-key": "1.3.0"
    },
    "devDependencies": {
        "@types/bn.js": "^5.1.0",
        "@types/chai": "^4.3.0",
        "@types/mocha": "^10.0.10",
        "@types/node": "^20.11.24",
        "chai": "^4.3.4",
        "mocha": "^9.0.3",
        "prettier": "^2.6.2",
        "ts-mocha": "^10.0.0",
        "typescript": "^4.3.5"
    }
}
