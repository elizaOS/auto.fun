{
    "license": "ISC",
    "scripts": {
        "lint:fix": "prettier */*.js \"*/**/*{.js,.ts}\" -w",
        "lint": "prettier */*.js \"*/**/*{.js,.ts}\" --check",
        "build": "RUSTUP_TOOLCHAIN=nightly-2024-11-19 anchor build",
        "deploy:dev": "RUSTUP_TOOLCHAIN=nightly-2024-11-19 anchor deploy --program-name raydium_vault --provider.cluster Devnet",
        "deploy:main": "RUSTUP_TOOLCHAIN=nightly-2024-11-19 anchor deploy --program-name raydium_vault --provider.cluster Mainnet",
        "deploy_buffer:main": "solana program deploy --buffer recover.json --url https://staked.helius-rpc.com?api-key=3e7503c3-c6b2-44ac-98d4-c309cbcb4074 target/deploy/raydium_vault.so",
        "deploy_buffer:dev": "solana program deploy --buffer recover.json --url devnet target/deploy/raydium_vault.so",
        "test:dev": "RUSTUP_TOOLCHAIN=nightly-2024-11-19 anchor run --provider.cluster Devnet test",
        "test:main": "RUSTUP_TOOLCHAIN=nightly-2024-11-19 anchor run --provider.cluster Mainnet test",
        "init:main": "RUSTUP_TOOLCHAIN=nightly-2024-11-19 anchor run --provider.cluster Mainnet test_init",
        "init:dev": "RUSTUP_TOOLCHAIN=nightly-2024-11-19 anchor run --provider.cluster Devnet test_init",
        "withdraw:main": "RUSTUP_TOOLCHAIN=nightly-2024-11-19 anchor run --provider.cluster Mainnet test_withdraw",
        "withdraw:dev": "RUSTUP_TOOLCHAIN=nightly-2024-11-19 anchor run --provider.cluster Devnet test_withdraw",
        "claim:main": "RUSTUP_TOOLCHAIN=nightly-2024-11-19 anchor run --provider.cluster Mainnet test_claim",
        "claim:dev": "RUSTUP_TOOLCHAIN=nightly-2024-11-19 anchor run --provider.cluster Devnet test_claim",
        "position:main": "RUSTUP_TOOLCHAIN=nightly-2024-11-19 anchor run --provider.cluster Mainnet test_nft_balance",
        "position:dev": "RUSTUP_TOOLCHAIN=nightly-2024-11-19 anchor run --provider.cluster Devnet test_nft_balance",
        "changeClaimer:main": "RUSTUP_TOOLCHAIN=nightly-2024-11-19 anchor run --provider.cluster Mainnet test_change_claimer",
        "changeClaimer:dev": "RUSTUP_TOOLCHAIN=nightly-2024-11-19 anchor run --provider.cluster Devnet test_change_claimer",
        "deposit:main": "RUSTUP_TOOLCHAIN=nightly-2024-11-19 anchor run --provider.cluster Mainnet test_deposit",
        "deposit:dev": "RUSTUP_TOOLCHAIN=nightly-2024-11-19 anchor run --provider.cluster Devnet test_deposit",
        "balance:main": "solana balance -um",
        "balance:dev": "solana balance -ud",
        "recover": "solana-keygen recover -o recover.json",
        "clear": "rm -r recover.json"
    },
    "dependencies": {
        "@coral-xyz/anchor": "^0.30.1",
        "@raydium-io/raydium-sdk-v2": "^0.1.90-alpha",
        "@solana/spl-token": "^0.4.13"
    },
    "devDependencies": {
        "@solana/web3.js": "^1.98.0",
        "@types/bn.js": "^5.1.0",
        "@types/chai": "^4.3.0",
        "@types/mocha": "^9.0.0",
        "chai": "^4.3.4",
        "mocha": "^9.0.3",
        "prettier": "^2.6.2",
        "ts-mocha": "^10.0.0",
        "typescript": "^4.3.5"
    }
}
