{
  "name": "auto.fun",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "postinstall": "cd vendor/udf && npm install",
    "dev": "npm run copy-dotenv && export NODE_ENV=development && npm run migrate:local && concurrently --kill-others-on-fail --names \"vite,worker,cron,vanity,webhook\" --prefix-colors \"blue,green,yellow,magenta,cyan\" \"vite\" \"wrangler dev --env development --test-scheduled\" \"npm run dev:cron-watch\" \"cd vanity && wrangler dev --env development --test-scheduled\" \"npm run dev:mock-helius-webhook\"",
    "dev:cron-watch": "node scripts/cron-watcher.js",
    "dev:helius-webhook": "node scripts/setup-webhook.js",
    "dev:mock-helius-webhook": "node scripts/mock-helius-webhook.js",
    "copy-dotenv": "[ -f \".env\" ] && (echo '# AUTOGENERATED FROM .env\n' > .dev.vars && cat .env >> .dev.vars) || (echo \"Error: .env file not found\" && exit 1)",
    "build": "tsc && vite build && cd vendor/udf && npm run build",
    "lint": "prettier --cache --write \"src/**/*.{ts,tsx}\" \"worker/**/*.{ts,tsx}\" && eslint ./src --fix && eslint ./worker --fix",
    "preview": "vite preview",
    "start": "concurrently \"npm run dev\" \"npm run worker:dev\"",
    "start:frontend": "npm run build && serve dist",
    "migrate:local": "wrangler d1 execute DB --file=./drizzle/migrations.sql --local --env development",
    "migrate:dev": "wrangler d1 execute DB --file=./drizzle/migrations.sql --remote --env development",
    "migrate:prod": "wrangler d1 execute DB --file=./drizzle/migrations.sql --remote",
    "generate": "drizzle-kit generate:sqlite",
    "deploy:worker": "wrangler deploy",
    "deploy:pages": "npm run build && wrangler pages deploy dist",
    "test": "vitest run worker/tests",
    "test:worker": "vitest run worker/tests",
    "test:routes": "vitest run worker/tests/routes",
    "test:routes:debug": "DEBUG=true vitest run worker/tests/routes",
    "test:auth": "vitest run worker/tests/unit/auth.test.ts",
    "test:token": "vitest run worker/tests/routes/token.test.ts",
    "setup:test-env": "./scripts/setup-test-env.sh",
    "run-cron": "curl \"http://localhost:8787/__scheduled?cron=*+*+*+*+*\""
  },
